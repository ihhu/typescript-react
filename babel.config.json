{
    "presets":[
        ["@babel/preset-env",{
            "useBuiltIns":"usage",
            "corejs":3
        }],
        "@babel/preset-react",
        "linaria/babel",
        "@babel/preset-typescript"
    ],
    "plugins":[
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-dynamic-import",
        ["@babel/plugin-transform-runtime",{
            "useESModules":true,
            "corejs":3
        }],
        [
            "import",{
                "libraryName": "antd",
                "libraryDirectory": "es",
                "style": true
            }
        ]
    ]
}